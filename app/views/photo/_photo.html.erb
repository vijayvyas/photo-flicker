<% @photos.each do |photo| %>
<div class="card">
  <img class="card-img-top" src=<%= photo.url %> alt="<%= photo.username %>">
  <div class="card-block">
    <h4 class="card-title"><%= photo.username %></h4>
    <p class="card-text"><%= photo.caption %></p>
    <a href="#" class="btn btn-primary"><span class="glyphicon glyphicon-heart"></span>Likes</a>
  </div>
</div>
<% end %>
